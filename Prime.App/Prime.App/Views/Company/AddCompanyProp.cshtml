@model Prime.App.Models.Company

@{
    ViewBag.Title = "Add Companies Prop";
}

<div class="form-horizontal">
    <hr />
    <div class="Col-md-12">
        <form asp-controller="Company" asp-action="AddCompanyProp" method="post">
            <div class="form-group">
                <div class="row">
                    <div class="form-group">
                        <label class="control-label">Property Name</label>
                        <input name="PropName" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Properety Value</label>
                        <input name="PropValue" class="form-control" />
                    </div>
                    <br /><br />
                    <div class="form-group">
                        <button type="submit" value="Save">Submit</button>
                    </div>
                </div>
            </div>

        </form>
    </div>
</div>
